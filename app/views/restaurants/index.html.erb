<div class="container">
<h1>Restaurants#index</h1>
<p>Find me in app/views/restaurants/index.html.erb</p>
  <ul>
    <% @restaurants.each do |restaurant| %>
      <li>Go to: <%= link_to restaurant.name , restaurant_path(restaurant.id) %></li>
    <% end %>
  </ul>
<%= link_to "Make a new Restaurant", new_restaurant_path %>
</div>
